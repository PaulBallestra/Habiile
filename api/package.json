{
  "name": "tmplate-api",
  "version": "0.0.1",
  "scripts": {
    "start": "TZ='Europe/Paris' nodemon --exec babel-node src/index.js",
    "start:production": "TZ='Europe/Paris' node --optimize_for_size --gc_interval=100 dist/index.js",
    "build": "npm run rm-build-prod && npm run build-prod",
    "build:test": "npm run build-prod",
    "build-prod": "babel src --out-dir dist --copy-files",
    "rm-build-prod": "rm -rf dist",
    "test": "NODE_ENV=test node_modules/.bin/nyc --reporter=lcov node_modules/.bin/mocha",
    "logs:production": "heroku logs --app rvbprod --tail",
    "logs:staging": "heroku logs --app rvbstaging --tail"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "async": "^3.2.0",
    "awesome-phonenumber": "^2.44.0",
    "aws-sdk": "^2.960.0",
    "axios": "^0.21.4",
    "base64-stream": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "csv-parse": "^4.14.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^8.6.0",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "geolib": "^3.3.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "koa-body": "^4.0.6",
    "koa-ejs": "^4.3.0",
    "koa-locales": "^1.12.0",
    "koa-session": "^6.2.0",
    "koa-smart": "^2.2.0",
    "lodash": "^4.17.20",
    "mime": "^2.5.2",
    "moment": "^2.29.1",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-transforms": "^2.0.0",
    "sib-api-v3-sdk": "^8.0.2",
    "slugify": "^1.6.5",
    "stream": "^0.0.2",
    "stripe": "^11.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.26.0",
    "chai": "^4.3.4",
    "eslint": "^7.13.0",
    "expect": "^26.6.2",
    "mocha": "^8.4.0",
    "nock": "^13.2.1",
    "nodemon": "^2.0.6",
    "prettier-eslint": "^11.0.0"
  }
}
